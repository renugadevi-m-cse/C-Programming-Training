//Basic Arithmetic Operations
#include <stdio.h>
int main()
{
    int a,b;
    scanf("%d",&a);
    scanf("%d",&b);
    printf("Sum: %d\nDifference: %d\nProduct: %d\nQuotient: %d\nRemainder: %d",a+b,a-b,a*b,a/b,a%b);
    return 0;
}

//Smart Shopper
#include <stdio.h>
int main()
{
    int c1,q1,c2,q2,c3,q3,budget;
    float total,tax_total;
    scanf("%d %d",&c1,&q1);
    scanf("%d %d",&c2,&q2);
    scanf("%d %d",&c3,&q3);
    scanf("%d",&budget);
    total=((c1*q1)+(c2*q2)+(c3*q3));
    printf("Total (Before Tax): %.2f\n",total);
    tax_total=(c1*q1+(c1*q1*10/100))+(c2*q2+(c2*q2*10/100))+(c3*q3+(c3*q3*10/100));
    printf("Total (After Tax): %.2f\n",tax_total);
    if(tax_total<+budget)
    {
        printf("Within Budget\n");
    }
    else
    {
        printf("Over Budget\n");
    }
    return 0;
}

//Greatest of Three Numbers
#include <stdio.h>
int main()
{
    int a,b,c;
    scanf("%d\n%d\n%d",&a,&b,&c);
    if(a>b && a>c)
    {
        printf("%d is the greatest",a);
    }
    else if(b>a && b>c)
    {
        printf("%d is the greatest",b);
    }
    else if(c>a && c>b)
    {
        printf("%d is the greatest",c);
    }
    else
    {
        printf("There is a tie for the greatest number");
    }
    return 0;
}

//Character Classifier
#include <stdio.h>
int main()
{
    char ch;
    scanf(" %c",&ch);
    if(ch>='A' && ch<='Z')
    {
        printf("Uppercase Alphabet");
    }
    else if(ch>='a' && ch<='z')
    {
        printf("Lowercase Alphabet");
    }
    else if(ch>='0' && ch<='9')
    {
        printf("Digit");
    }
    else
    {
        printf("Special Character");
    }
    return 0;
}

//Triangle Type Detector
#include <stdio.h>
int main()
{
    
    int a,b,c;
    scanf("%d %d %d",&a,&b,&c);
    if((a+b)>c)
    {
        if(a==b && b==c)
        {
            printf("Equilateral Triangle");
        }
        else if((a==b || b==c) && a!=c)
        {
            printf("Isosceles Triangle");
        }
        else
        {
            printf("Scalene Triangle");
        }
    }
    else
    {
        printf("Not a valid triangle");
    }
    return 0;
}

//Leap Year Checker
#include <stdio.h>
int main()
{
    int year;
    scanf("%d",&year);
    if((year%400==0) ||(year%4==0 && year%100!=0))
    {
        printf("Leap Year");
    }
    else
    {
        printf("Not a Leap Year");
    }
    return 0;
}

//Print All Factors
#include <stdio.h>
int main()
{
    int N,i;
    scanf("%d",&N);
    for(i=1;i<=N;i++)
    {
       if(N%i==0)
       {
           printf("%d ",i);
       }
    }
    return 0;
}

//Is It a Prime Number?
#include <stdio.h>
int main()
{
    int n,i,count=0;
    scanf("%d",&n);
    if(n<=1)
    {
        printf("Not Prime");
        return 0;
    }
    for(i=2;i<=n/2;i++)
    {
        if(n%i==0)
        {
            count=1;
            break;
        }
    }
    if(count!=1)
    {
        printf("Prime");
    }
    else
    {
        printf("Not Prime");
    }
    return 0;
}

